{
  "name": "emi-discord-bot",
  "version": "1.0.1",
  "description": "An open-source, multipurpose discord bot built using discord-js",
  "main": "bot.js",
  "author": "N0rule",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "nodemon .",
    "start": "node .",
    "dev:win": "chcp 65001 && nodemon .",
    "start:win": "chcp 65001 && node .",
    "format": "prettier --write src",
    "docker:package": "tar -cf discord-js-bot.tar dashboard logs src bot.js config.js dockerfile package.json package-lock.json",
    "docker:build": "docker build -t saitejamadha/discord-js-bot:5.5.0 .",
    "postinstall": "node scripts/lavaclientPluginQueueFix.js"
  },
  "homepage": "https://github.com/N0rule/Emi-discord-bot#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/N0rule/emi-discord-bot.git"
  },
  "bugs": {
    "url": "https://github.com/N0rule/emi-discord-bot/issues"
  },
  "dependencies": {
    "@lavaclient/plugin-queue": "^0.0.1",
    "@vitalets/google-translate-api": "^9.2.0",
    "ascii-table": "0.0.9",
    "btoa": "^1.2.1",
    "common-tags": "^1.8.2",
    "connect-mongo": "^5.1.0",
    "country-emoji-languages": "^1.0.0",
    "discord-gamecord": "^4.4.0",
    "discord-giveaways": "^6.0.1",
    "discord-together": "^1.3.31",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "enhanced-ms": "^2.3.0",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "fixedsize-map": "^1.0.1",
    "iso-639-1": "^3.1.0",
    "lavaclient": "^5.0.0-rc.3",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "mongoose": "^8.1.1",
    "nekos.life": "^3.0.0",
    "node-fetch": "^2.7.0",
    "node-osu": "^2.2.1",
    "openai": "^3.2.1",
    "os": "^0.1.2",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.1",
    "pretty-ms": "^7.0.1",
    "sourcebin_js": "^0.0.3-ignore",
    "string-progressbar": "^1.0.4",
    "table": "^6.8.1",
    "timestamp-to-date": "^1.1.0",
    "twemoji-parser": "^14.0.0",
    "vrc-ts": "^1.0.13"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "erlpack": "^0.1.4",
    "utf-8-validate": "^6.0.3",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-plugin-jsdoc": "^46.4.3",
    "node": "^18.18.2",
    "nodemon": "^3.0.3",
    "prettier": "3.2.5"
  },
  "keywords": [
    "discord",
    "discord.js",
    "discord-bot",
    "open-source",
    "bot",
    "multipurpose",
    "multi-purpose-bot"
  ],
  "_moduleAliases": {
    "@root": ".",
    "@handlers": "src/handlers/",
    "@helpers": "src/helpers/",
    "@schemas": "src/database/schemas/",
    "@src": "src/",
    "@structures": "src/structures/"
  }
}
